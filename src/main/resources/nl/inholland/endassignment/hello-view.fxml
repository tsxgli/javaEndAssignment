    <PasswordField fx:id="txtPassword" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="130.0" promptText="Enter password" GridPane.rowIndex="1">
      <GridPane.margin>
         <Insets left="40.0" />
      </GridPane.margin></PasswordField>
   <Label fx:id="testLabel" GridPane.rowIndex="2" />
</GridPane>
